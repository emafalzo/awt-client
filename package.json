{
  "name": "AWT-Project",
  "description": "An example Web Client application generated by IFML-WebEditor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "gulp",
    "postinstall": "npm run build",
    "start": "pm2 start --name=AWT-Project index.js",
    "poststart": "node -e \"require('open')('http://localhost:3000')\"",
    "stop": "pm2 stop AWT-Project"
  },
  "devDependencies": {
    "open": "0.0.5",
    "pm2": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "bootstrap": "^3.3.6",
    "bootstrap-notify": "^3.1.3",
    "browserify": "^13.0.0",
    "exposify": "^0.5.0",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.2.0",
    "gulp-uglify": "^1.5.1",
    "jquery": "^2.2.0",
    "knockout": "^3.4.0",
    "merge-stream": "^1.0.0",
    "nedb": "^1.8.0",
    "stringify": "^5.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
